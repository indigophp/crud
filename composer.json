{
    "name": "indigophp/crud",
    "description": "Basic CRUD stuff",
    "license": "MIT",
    "keywords": ["CRUD"],
    "homepage": "https://indigophp.com",
    "authors": [
        {
            "name": "Márk Sági-Kazár",
            "email": "mark.sagikazar@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4",
        "league/tactician": "^0.5"
    },
    "require-dev": {
        "phpspec/phpspec": "~2.1",
        "henrikbjorn/phpspec-code-coverage" : "~1.0"
    },
    "autoload": {
        "psr-4": {
            "Indigo\\Crud\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Indigo\\Crud\\Stub\\": "stubs/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1-dev"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev"
}
